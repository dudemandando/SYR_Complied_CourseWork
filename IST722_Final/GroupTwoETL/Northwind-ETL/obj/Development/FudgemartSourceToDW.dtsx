                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        fdc.CustomerState AS State, COUNT(fdo.OrdersID) AS TotalStateOrders, MIN(fdpr.ReviewStars) AS MinReview, MAX(fdpr.ReviewStars) AS MaxReview, AVG(fdpr.ReviewStars) AS AverageProductReview, SUM(fdo.TotalRetail) 
